{
	"name": "slp-simple-blog-react",
	"version": "1.0.1",
	"private": true,
	"description": "Simple blog application to learn the basics of React.js.",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"serve": "json-server --watch db.json --port 5000",
		"dev": "concurrently \"yarn start\" \"yarn serve\""
	},
	"dependencies": {
		"@babel/core": "^7.17.9",
		"@babel/plugin-syntax-flow": "^7.16.7",
		"@babel/plugin-transform-react-jsx": "^7.17.3",
		"axios": "^0.26.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.2.2",
		"react-scripts": "5.0.0",
		"typescript": "^4.6.3"
	},
	"devDependencies": {
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.0",
		"@tailwindcss/line-clamp": "^0.3.1",
		"@tailwindcss/typography": "^0.5.2",
		"autoprefixer": "^10.4.4",
		"concurrently": "^7.0.0",
		"json-server": "^0.17.0",
		"postcss": "^8.4.12",
		"prettier": "^2.6.1",
		"prettier-plugin-tailwindcss": "^0.1.8",
		"tailwindcss": "^3.0.23"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"author": {
		"name": "Michael Owuor"
	}
}
